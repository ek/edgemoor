package wolken.edgemoor {		import flash.events.Event;	import flash.events.EventDispatcher;			public class EdgemoorConfig extends EventDispatcher {			private var _edgemoorXML:EdgemoorXML;				public var introFrames:Array;		public var states:Array;		public var playIntroWhenLoaded:int = 2;		public var delayTime:int = 0;				public function EdgemoorConfig(xmlURL) {						introFrames = new Array();			states = new Array();						_edgemoorXML = new EdgemoorXML( xmlURL, this );						_edgemoorXML.addEventListener( Event.COMPLETE, xmlComplete );					}				private function xmlComplete(event:Event):void {			// xml is done loading and populating the config data			dispatchEvent(new Event(Event.COMPLETE));		}		}}